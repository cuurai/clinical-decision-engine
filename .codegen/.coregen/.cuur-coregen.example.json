{
  "$schema": "./schema.json",
  "domains": [
    {
      "name": "exchange",
      "enabled": true,
      "spec_path": "exchange.yaml",
      "bundled_path": "exchange.json"
    },
    {
      "name": "auth",
      "enabled": true
    },
    {
      "name": "governance",
      "enabled": true
    }
  ],
  "paths": {
    "project_root": "../..",
    "openapi_dir": "../../openapi/src",
    "bundled_dir": "../../openapi/src/.bundled"
  },
  "layers": {
    "core": {
      "handlers": {
        "enabled": true,
        "include_js_docs": true,
        "include_validation": true
      },
      "types": {
        "enabled": true,
        "re_export_generated_types": true
      },
      "validators": {
        "enabled": true,
        "base_url": "https://api.cuur.ai/v2"
      },
      "converters": {
        "enabled": true
      },
      "include_repositories": true,
      "include_validation": true,
      "include_converters": true
    },
    "sdk": {
      "enabled": true,
      "generate_types": true,
      "generate_schemas": true,
      "generate_clients": false,
      "base_url": "https://api.cuur.ai/v2"
    }
  },
  "generators": {
    "handler": {
      "enabled": true,
      "include_js_docs": true,
      "namespace_exports": false
    },
    "repository": {
      "enabled": true,
      "validate_entity_types": true
    },
    "schema": {
      "enabled": true,
      "include_validation": true
    },
    "types": {
      "enabled": true,
      "filter_shared_types": true
    }
  },
  "pipeline": {
    "clean": false,
    "validate": true,
    "skip_build": false,
    "parallel": false,
    "fail_fast": true,
    "rollback": false
  },
  "build": {
    "run_after_generate": true,
    "packages": [
      "@cuur/core"
    ]
  },
  "git": {
    "auto_commit": false,
    "commit_message": "chore: regenerate code"
  },
  "log_level": "info",
  "verbose": false,
  "version": "1.0.0"
}