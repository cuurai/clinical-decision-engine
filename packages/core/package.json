    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@zodios/core": "^10.9.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
