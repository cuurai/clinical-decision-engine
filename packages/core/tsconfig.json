{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "verbatimModuleSyntax": false,
    "baseUrl": ".",
    "emitDeclarationOnly": false
  },
  "exclude": [
    "node_modules",
    "dist",
    "**/dist/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.js",
    "**/*.d.ts",
    "**/*.js.map",
    "**/*.d.ts.map"
  ],
  "include": []
}
